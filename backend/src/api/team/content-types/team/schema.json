{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "teams"
    },
    "sport": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sport.sport",
      "inversedBy": "teams"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "female",
        "male"
      ]
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "beginner",
        "amature",
        "profesional"
      ]
    },
    "slogan": {
      "type": "text"
    },
    "matches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::individuals-match.individuals-match",
      "mappedBy": "teams"
    },
    "match_scores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match-score.match-score",
      "mappedBy": "team"
    },
    "league_championship": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::league-champion.league-champion",
      "mappedBy": "team"
    },
    "leagues": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::individuals-league.individuals-league",
      "mappedBy": "team_type_members"
    }
  }
}
