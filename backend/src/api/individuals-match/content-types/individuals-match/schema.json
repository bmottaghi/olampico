{
  "kind": "collectionType",
  "collectionName": "individuals_matches",
  "info": {
    "singularName": "individuals-match",
    "pluralName": "individuals-matches",
    "displayName": "Match",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "individual",
        "team"
      ]
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "matches"
    },
    "datetime": {
      "type": "datetime"
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "inversedBy": "matches"
    },
    "match_scores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match-score.match-score",
      "mappedBy": "match"
    },
    "match_status": {
      "type": "enumeration",
      "enum": [
        "finished",
        "canceled"
      ]
    }
  }
}
