{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "female",
        "male"
      ],
      "required": true
    },
    "email": {
      "type": "email",
      "required": false
    },
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "player_skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player-skill.player-skill",
      "mappedBy": "player"
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "mappedBy": "players"
    },
    "individuals_matches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::individuals-match.individuals-match",
      "mappedBy": "players"
    },
    "individuals_leagues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::individuals-league.individuals-league",
      "mappedBy": "player_champion"
    },
    "player_match_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::individuals-match.individuals-match",
      "mappedBy": "player_winner"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "player"
    }
  }
}
